text = "Welcome to Python Programming 2026"

letters = 0
digits = 0
spaces = 0

for ch in text:
    if ch.isalpha():
        letters += 1
    elif ch.isdigit():
        digits += 1
    elif ch.isspace():
        spaces += 1

print("Text:", text)
print("Letters:", letters)
print("Digits:", digits)
print("Spaces:", spaces)

#output
Text: Welcome to Python Programming 2026
Letters: 28
Digits: 4
Spaces: 4
